; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="ShortServe"
run/main_scene="res://Data/Scenes/Splash.tscn"
boot_splash/image="res://Assets/Art/splash_loading.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

App="*res://App/App.cs"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false

[editor_plugins]

enabled=PoolStringArray( "MonoDebuggerQM" )

[mono]

debugger_agent/wait_timeout=10000

[rendering]

environment/default_clear_color=Color( 0.780392, 0.780392, 0.780392, 1 )
environment/default_environment="res://default_env.tres"
